/**app.wxss**/

@import "/styles/icon";
.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.page,
view,
text,
swiper,
swiper-item,
rich-text,
navigator {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}